pipeline {
    agent any

    stages {
        
        stage('Build') {
            steps {
                echo 'Building.....'
                dir('PipelineSource\\Pipeline') {
                    bat 'mvn clean compile'  
                    }
                
            }
        }
         stage('Test') {
            steps {
                echo 'Running Tests.....'
                dir('PipelineSource\\Pipeline') {
                     bat 'mvn clean test -P dev'  
                    }
               
            }
        }
    }
}
