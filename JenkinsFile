pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building.....'
                dir('PipelineSource\\Pipeline') {
                    bat 'mvn clean compile'  
                    }
                
            }
        }
         stage('Test') {
            steps {
                echo 'Running Tests.....'
                dir('PipelineSource\\Pipeline') {
                     bat 'mvn clean package -P qa' 
                    }
               
            }
            
            stage('Deploy') {
            steps {
                echo 'Running Deploy.....'
                echo ${JENKINS_HOME}
            }
        }
    }
}